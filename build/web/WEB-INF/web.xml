<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControlAcceso</servlet-name>
        <servlet-class>controlador.ControlAcceso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlUsuario</servlet-name>
        <servlet-class>controlador.ControlUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlMedico</servlet-name>
        <servlet-class>controlador.ControlMedico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlEspecialidad</servlet-name>
        <servlet-class>controlador.ControlEspecialidad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlMedico01</servlet-name>
        <servlet-class>controlador.ControlMedico01</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlPaciente</servlet-name>
        <servlet-class>controlador.ControlPaciente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlCita</servlet-name>
        <servlet-class>controlador.ControlCita</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControlAcceso</servlet-name>
        <url-pattern>/ControlAcceso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlUsuario</servlet-name>
        <url-pattern>/ControlUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlMedico</servlet-name>
        <url-pattern>/ControlMedico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlEspecialidad</servlet-name>
        <url-pattern>/ControlEspecialidad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlMedico01</servlet-name>
        <url-pattern>/ControlMedico01</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlPaciente</servlet-name>
        <url-pattern>/ControlPaciente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlCita</servlet-name>
        <url-pattern>/ControlCita</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
